#!/usr/bin/env rash
- name: Simple GET request to check if a website is accessible
  uri:
    url: https://httpbin.org/get
    method: GET

- name: GET request with content return and registration
  uri:
    url: https://httpbin.org/get
    method: GET
    return_content: true
  register: get_response

- name: Display the response content
  debug:
    var: get_response
